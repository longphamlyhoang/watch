<!DOCTYPE html>
<html lang="en-US" dir="ltr">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--
    Document Title
    =============================================
    -->
    <title>@ViewBag.Title</title>
    <!--
    Favicons
    =============================================
    -->
    <link rel="apple-touch-icon" sizes="57x57" href="~/assets-shop/images/favicons/apple-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="~/assets-shop/images/favicons/apple-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="~/assets-shop/images/favicons/apple-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="~/assets-shop/images/favicons/apple-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="~/assets-shop/images/favicons/apple-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="~/assets-shop/images/favicons/apple-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="~/assets-shop/images/favicons/apple-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="~/assets-shop/images/favicons/apple-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="~/assets-shop/images/favicons/apple-icon-180x180.png">
    <link rel="icon" type="image/png" sizes="192x192" href="~/assets-shop/images/favicons/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="~/assets-shop/images/favicons/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="~/assets-shop/images/favicons/favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="~/assets-shop/images/favicons/favicon-16x16.png">
    <link rel="manifest" href="/manifest.json">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="~/assets-shop/images/favicons/ms-icon-144x144.png">
    <meta name="theme-color" content="#ffffff">
   
    <link href="~/assets-shop/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <!-- Template specific stylesheets-->
    <link href="https://fonts.googleapis.com/css?family=Roboto+Condensed:400,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Volkhov:400i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800" rel="stylesheet">
    <link href="~/assets-shop/lib/animate.css/animate.css" rel="stylesheet">
    <link href="~/assets-shop/lib/components-font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="~/assets-shop/lib/et-line-font/et-line-font.css" rel="stylesheet">
    <link href="~/assets-shop/lib/flexslider/flexslider.css" rel="stylesheet">
    <link href="~/assets-shop/lib/owl.carousel/dist/~/assets-shop/owl.carousel.min.css" rel="stylesheet">
    <link href="~/assets-shop/lib/owl.carousel/dist/~/assets-shop/owl.theme.default.min.css" rel="stylesheet">
    <link href="~/assets-shop/lib/magnific-popup/dist/magnific-popup.css" rel="stylesheet">
    <link href="~/assets-shop/lib/simple-text-rotator/simpletextrotator.css" rel="stylesheet">
    <!-- Main stylesheet and color file-->
    <link href="~/assets-shop/css/style.css" rel="stylesheet">
    <link id="color-scheme" href="~/assets-shop/css/colors/default.css" rel="stylesheet">
</head>
<body data-spy="scroll" data-target=".onpage-navigation" data-offset="60">
    <main>
        <div class="page-loader">
            <div class="loader">Loading...</div>
        </div>
        <nav class="navbar navbar-custom navbar-fixed-top navbar-transparent" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#custom-collapse"><span class="sr-only">Toggle navigation</span><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span></button><a class="navbar-brand" href="index.html">Titan</a>
                </div>
                <div class="collapse navbar-collapse" id="custom-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown">Login</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <section class="home-section home-fade home-full-height" id="home">
            <div class="hero-slider">
                <ul class="slides">
                    <li class="bg-dark-30 bg-dark shop-page-header" style="background-image:url(&quot;~/assets-shop/images/shop/slider1.png&quot;);">
                        <div class="titan-caption">
                            <div class="caption-content">
                                <div class="font-alt mb-30 titan-title-size-1">This is Titan</div>
                                <div class="font-alt mb-30 titan-title-size-4"> Summer 2021</div>
                                <div class="font-alt mb-40 titan-title-size-1">Your online fashion destination</div>
                            </div>
                        </div>
                    </li>
                    <li class="bg-dark-30 bg-dark shop-page-header" style="background-image: url(&quot;~/Images/banner.jpg.jpg); ">
                        <div class="titan-caption">
                            <div class="caption-content">
                                <div class="font-alt mb-30 titan-title-size-1"> This is Titan</div>
                                <div class="font-alt mb-40 titan-title-size-4">Exclusive products</div>
                            </div>
                        </div>
                    </li>
                </ul>
            </div>
        </section>
        <div class="main">
            @RenderBody()
            <footer class="footer bg-dark">
                <div class="container">
                    <div class="row">
                        <div class="col-sm-6">
                            <p class="copyright font-alt">&copy; 2017&nbsp;<a href="index.html">TitaN</a>, All Rights Reserved</p>
                        </div>
                        <div class="col-sm-6">
                            <div class="footer-social-links">
                                <a href="#"><i class="fa fa-facebook"></i></a><a href="#"><i class="fa fa-twitter"></i></a><a href="#"><i class="fa fa-dribbble"></i></a><a href="#"><i class="fa fa-skype"></i></a>
                            </div>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
        <div class="scroll-up"><a href="#totop"><i class="fa fa-angle-double-up"></i></a></div>
    </main>
    <!--
    JavaScripts
    =============================================
    -->
    <script src="~/assets-shop/lib/jquery/dist/jquery.js"></script>
    <script src="~/assets-shop/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/assets-shop/lib/wow/dist/wow.js"></script>
    <script src="~/assets-shop/lib/jquery.mb.ytplayer/dist/jquery.mb.YTPlayer.js"></script>
    <script src="~/assets-shop/lib/isotope/dist/isotope.pkgd.js"></script>
    <script src="~/assets-shop/lib/imagesloaded/imagesloaded.pkgd.js"></script>
    <script src="~/assets-shop/lib/flexslider/jquery.flexslider.js"></script>
    <script src="~/assets-shop/lib/owl.carousel/dist/owl.carousel.min.js"></script>
    <script src="~/assets-shop/lib/smoothscroll.js"></script>
    <script src="~/assets-shop/lib/magnific-popup/dist/jquery.magnific-popup.js"></script>
    <script src="~/assets-shop/lib/simple-text-rotator/jquery.simple-text-rotator.min.js"></script>
    <script src="~/assets-shop/js/plugins.js"></script>
    <script src="~/assets-shop/js/main.js"></script>
</body>
</html>